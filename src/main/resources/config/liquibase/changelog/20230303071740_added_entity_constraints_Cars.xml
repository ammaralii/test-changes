        <addForeignKeyConstraint baseColumnNames="color_id"
                                 baseTableName="rel_cars__color"
                                 constraintName="fk_rel_cars__color__color_id"
                                 referencedColumnNames="id"
                                 referencedTableName="colors"/>
    </changeSet>
</databaseChangeLog>
